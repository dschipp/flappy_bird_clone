{
    "version":1,
    "root":{
        "handlers" : ["log_handler"],
        "level": "DEBUG"
    },
    "handlers":{
        "log_handler":{
            "formatter": "std_out",
            "class": "logging.FileHandler",
            "level": "DEBUG",
            "filename":"app.log"
        }
    },
    "formatters":{
        "std_out": {
            "format": "%(asctime)s : %(levelname)s : %(module)s : %(funcName)s : %(lineno)d : %(message)s",
            "datefmt":"%d-%m-%Y %I:%M:%S"
        }
    }
}